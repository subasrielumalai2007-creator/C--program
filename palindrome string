#include <stdio.h>
#include <string.h>

int main() {
    char s[20], r[20];
    int i, j = 0, len;

    printf("Enter string: ");
    scanf("%s", s);

    len = strlen(s);
    for (i = len - 1; i >= 0; i--)
        r[j++] = s[i];
    r[j] = '\0';

    if (strcmp(s, r) == 0)
        printf("Palindrome");
    else
        printf("Not Palindrome");

    return 0;
}

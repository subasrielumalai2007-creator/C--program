#include <stdio.h>
int main() {
    int cp, sp;
    printf("Enter cost price and selling price: ");
    scanf("%d%d", &cp, &sp);

    if(sp > cp)
        printf("Profit = %d", sp-cp);
    else if(cp > sp)
        printf("Loss = %d", cp-sp);
    else
        printf("No profit no loss");
    return 0;
}

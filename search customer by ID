#include <stdio.h>
struct customer {
    int id;
    char name[20];
};
int main() {
    struct customer c[3];
    int i, sid;
    for(i=0;i<3;i++)
        scanf("%d %s",&c[i].id,c[i].name);

    printf("Enter ID to search: ");
    scanf("%d",&sid);

    for(i=0;i<3;i++)
        if(c[i].id == sid)
            printf("Name: %s",c[i].name);

    return 0;
}
